# Local development environment variables
NODE_ENV=development
ENV_NAME=local

# Database configuration
DATABASE_URL=postgres://postgres:postgres@localhost:5432/healthtracker

# AWS configuration (update these with your actual credentials)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
AWS_S3_BUCKET=healthtracker-local

# OpenAI API configuration
OPENAI_API_KEY=

# Stripe configuration
STRIPE_SECRET_KEY=
VITE_STRIPE_PUBLIC_KEY=
STRIPE_WEBHOOK_SECRET=

# Session secret (generate a random string)
SESSION_SECRET=local_development_session_secret